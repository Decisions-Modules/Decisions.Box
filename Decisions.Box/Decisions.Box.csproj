<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" Condition="Exists('$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props')" />
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>Library</OutputType>
    <RootNamespace>Decisions.Box</RootNamespace>
    <AssemblyName>$(RootNamespace)</AssemblyName>
    <RootPath>../../../</RootPath>
    
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="DecisionsFramework">
      <HintPath>$(RootPath)output\core\framework\DecisionsFramework.dll</HintPath>
    </Reference>
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="DecisionsSDK" Version="6.16.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>
